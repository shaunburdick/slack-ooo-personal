{
  "name": "slack-ooo-personal",
  "version": "1.2.0",
  "description": "Personal Out of Office Bot",
  "main": "app.js",
  "private": true,
  "scripts": {
    "test": "npm run lint && npm run unit",
    "unit": "nyc --all tape ./test/*.test.js | faucet && nyc report",
    "doc": "jsdoc -c ./.jsdocrc .",
    "start": "node app.js",
    "lint": "semistandard --verbose | snazzy",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/shaunburdick/slack-ooo-personal.git"
  },
  "author": "Shaun Burdick <git@shaunburdick.com>",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/shaunburdick/slack-ooo-personal/issues"
  },
  "homepage": "https://github.com/shaunburdick/slack-ooo-personal#readme",
  "engine": {
    "node": "^5.1.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "faucet": "0.0.1",
    "gulp": "^3.9.0",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.1",
    "jasmine-node": "^1.14.5",
    "jsdoc": "^3.4.0",
    "nyc": "^8.1.0",
    "semistandard": "^8.0.0",
    "snazzy": "^4.0.1",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "botkit": "^0.2.2",
    "redact-object": "^1.0.1",
    "winston": "^2.1.0"
  },
  "semistandard": {
    "ignore": [
      "coverage",
      "doc"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ]
  }
}
